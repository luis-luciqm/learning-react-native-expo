{
  "expo": {
    "name": "my-first-app",
    "slug": "my-first-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/orange.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/orange.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/orange.jpg",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.luislucilandio.myfirstapp"
    },
    "web": {
      "favicon": "./src/orange.jpg"
    }
  }
}
